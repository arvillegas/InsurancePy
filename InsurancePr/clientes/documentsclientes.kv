# Reference agentes.py
#: import main clientes.clientes
#: import ListAdapter kivy.adapters.listadapter.ListAdapter
#: import ListItemButton kivy.uix.listview.ListItemButton
 
DocumentosClientes:
 
<DocumentosClientes>:
    orientation: "vertical"
    padding: 10
    spacing: 10

 
    Label:
        text:'Documentos Clientes'
        pos:0,250
        font_size:50

      
    GridLayout:
        cols:2
        rows:3
        size_hint_y: None
        height: "100dp"
        pos: -100, 400
        Label:
            text:"Comprobante de Domicilio"
        Button:
            id:picture_file
            size_hint: (None, None)
            height: 30
            width: 200
            text:"Cargar Comprobante"
        Label:
            text:"INE"
        Button:
            id:picture_file
            size_hint: (None, None)
            height: 30
            width: 200
            text:"Cargar Comprobante"
        Label:
            text:"Comprobante de Ingresos"
        Button:
            id:picture_file
            size_hint: (None, None)
            height: 30
            width: 200
            text:"Cargar Comprobante"
            
    BoxLayout:
        size_hint_y: None
        height: "40dp"
        Button:
            text: "Guardar"
            size_hint_x: 15
            on_press: root.submit_student()
        Button:
            text: "Borrar"
            size_hint_x: 15
            on_press: root.delete_student()
        Button:
            text: "Regresar"
            size_hint_x: 15
            on_press: root.go_back()